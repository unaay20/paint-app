{
  "name": "paint-app",
  "version": "2.0.0",
  "description": "Aplicación web de dibujo vectorial con autenticación",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "sass": "sass --watch public/styles/style.scss:public/style.css",
    "sass:build": "sass public/style.scss:public/style.css --style compressed",
    "dev:all": "concurrently \"npm run sass\" \"npm run dev\""
  },
  "keywords": [
    "paint",
    "canvas",
    "drawing",
    "vector"
  ],
  "author": "U Náay",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "sass": "^1.69.5"
  }
}
